{
  "extends": "airbnb-base",
  "env":
  {
    "es6": true,
    "browser": true
  },

  "parserOptions":
  {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },

  "rules":
  {
    "linebreak-style": 0,
    "no-restricted-syntax":
    [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
 }
}
